
<div class="kontakt-for-test container storleden-module">
	<h6>{{ form.ingress | raw }}</h6>
 	<form.form {{form.form.attributes }} method="post">
                    {{ form.form.form.form.form.form_build_id }}
                    {{ form.form.form.form_token }}
                    {{ form.form.form.form_id }}
                    {{ form.form.subject }}
                    {{ form.form.message }}
	<div class="row">
		<div class="col-sm-6">
			{{ form.form.name }}
		</div>
		<div class="col-sm-6">
			{{ form.form.company }}
		</div>
	</div>
	<div class="row">
		<div class="col-sm-6">
			{{ form.form.email }}
		</div>
		<div class="col-sm-6">
			{{ form.form.phone }}
		</div>
	</div>
	<div class="submit-btn">
		{{ form.form.actions }}
	</div>
	</form.form>
</div>

